<jxb:bindings xmlns="https://jakarta.ee/xml/ns/jaxb"
              xmlns:jxb="https://jakarta.ee/xml/ns/jaxb"
              xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix"
              version="3.0">
    <bindings schemaLocation="../../../../target/downloaded-sources/xsd/kitodo.xsd">
        <schemaBindings>
            <package name="org.kitodo.dataformat.metskitodo"/>
        </schemaBindings>
        <bindings>
            <namespace:prefix name="kitodo"/>
            <namespace:prefix name="xsi" namespaceURI="http://www.w3.org/2001/XMLSchema-instance" />
            <namespace:prefix name="xlink" namespaceURI="http://www.w3.org/1999/xlink" />
        </bindings>
    </bindings>
</jxb:bindings>
