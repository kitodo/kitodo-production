<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.sub.goobi.Beans.Regelsatz" table="MetadatenKonfigurationen">
		<id name="id" column="MetadatenKonfigurationID" type="integer">
			<generator class="native"/>
		</id>
		
		<property name="titel" column="Titel" type="string"/>
		<property name="datei" column="Datei" type="string"/>
		<property name="orderMetadataByRulesetHibernate" column="orderMetadataByRuleset"/>
		
	</class>

</hibernate-mapping>
