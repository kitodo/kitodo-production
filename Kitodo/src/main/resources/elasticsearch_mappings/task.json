{
  "settings": {
     "analysis": {
       "normalizer": {
         "lowercase": {
           "type": "custom",
           "filter": ["lowercase"]
         }
       }
     }
   },
  "mappings": {
    "dynamic": "strict",
    "properties": {
      "batchStep": {
        "type": "boolean"
      },
      "homeDirectory": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256
          }
        }
      },
      "ordering": {
        "type": "long"
      },
      "processForTask": {
        "properties": {
          "id": {
            "type": "long"
          },
          "title": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256,
                "normalizer": "lowercase"
              }
            },
            "fielddata": true
          },
          "creationDate": {
            "type": "date",
            "format": "yyyy-MM-dd HH:mm:ss",
            "ignore_malformed": true
          }
        }
      },
      "clientForTask": {
        "type": "long"
      },
      "projectForTask": {
        "properties": {
          "id": {
            "type": "long"
          },
          "title": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          }
        }
      },
      "relatedProjectIds": {
        "type": "long"
      },
      "templateForTask": {
        "properties": {
          "id": {
            "type": "long"
          },
          "title": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            },
            "fielddata": true
          }
        }
      },
      "processingStatus": {
        "type": "long"
      },
      "editType": {
        "type": "long"
      },
      "processingTime": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss",
        "ignore_malformed": true
      },
      "processingBegin": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss",
        "ignore_malformed": true
      },
      "processingEnd": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss",
        "ignore_malformed": true
      },
      "processingUser": {
        "properties": {
          "id": {
            "type": "long"
          },
          "login": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            },
            "fielddata": true
          },
          "name": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            },
            "fielddata": true
          },
          "surname": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            },
            "fielddata": true
          },
          "fullname": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          }
        }
      },
      "title": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256,
            "normalizer": "lowercase"
          },
          "raw": {
            "type": "text",
            "index": false
          }
        },
        "fielddata": true
      },
      "correction": {
        "type": "boolean"
      },
      "correctionCommentStatus": {
        "type": "byte"
      },
      "typeAutomatic": {
        "type": "boolean"
      },
      "typeImagesRead": {
        "type": "boolean"
      },
      "typeImagesWrite": {
        "type": "boolean"
      },
      "typeMetadata": {
        "type": "boolean"
      },
      "roles": {
        "properties": {
          "id": {
            "type": "long"
          }
        }
      }
    }
  }
}
