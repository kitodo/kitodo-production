<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
	 <application>
	 	<locale-config>
	 	<default-locale>en</default-locale>
	 	<supported-locale>de</supported-locale>
	 	<supported-locale>en</supported-locale>
	 	<supported-locale>es</supported-locale>
	 	</locale-config>
	 </application>
   
	<!-- Startseite -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>newMain</from-outcome>
			<to-view-id>/newpages/Main.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- TechnischerHintergrund -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>TechnischerHintergrund</from-outcome>
			<to-view-id> /newpages/statischTechnischerHintergrund.jsp </to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Impressum -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>Impressum</from-outcome>
			<to-view-id>/newpages/statischImpressum.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bedienungshinweise -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>Bedienung</from-outcome>
			<to-view-id>/newpages/statischBedienung.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
  <!-- Taskmanager -->
  <navigation-rule>
    <navigation-case>
      <from-outcome>taskmanager</from-outcome>
      <to-view-id>/newpages/taskmanager.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
	
	 <!-- Bearbeitung von Benutzern -->
  <navigation-rule>
    <navigation-case>
      <from-outcome>modulemanager</from-outcome>
      <to-view-id>/newpages/modulemanager.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
	
		<!-- Bearbeitung von Benutzern -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzerBearbeiten</from-outcome>
			<to-view-id>/newpages/BenutzerBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle Benutzergruppen als Popup für die Bearbeitung der Benutzer-->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzerBearbeitenPopup</from-outcome>
			<to-view-id>/newpages/BenutzerBearbeitenPopup.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle Projekte als Popup für die Bearbeitung der Benutzer-->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzerBearbeitenPopupProjekte</from-outcome>
			<to-view-id>/newpages/BenutzerBearbeitenPopupProjekte.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle Benutzer -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzerAlle</from-outcome>
			<to-view-id>/newpages/BenutzerAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung von Benutzergruppen -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzergruppenBearbeiten</from-outcome>
			<to-view-id>/newpages/BenutzergruppenBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle Benutzergruppen -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BenutzergruppenAlle</from-outcome>
			<to-view-id>/newpages/BenutzergruppenAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle LdapGruppen -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>LdapGruppenAlle</from-outcome>
			<to-view-id>/newpages/LdapGruppenAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		
	<!-- Bearbeitung von LdapGruppen -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>LdapGruppenBearbeiten</from-outcome>
			<to-view-id>/newpages/LdapGruppenBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Benutzerkonfiguratino -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>Benutzerkonfiguration</from-outcome>
			<to-view-id>/newpages/BenutzerKonfiguration.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Passwort ändern -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>newPasswortAendern</from-outcome>
			<to-view-id>/newpages/PasswortAendern.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Administrationsaufgaben -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>Administrationsaufgaben</from-outcome>
			<to-view-id>/newpages/Administrationsaufgaben.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung von Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungSuche</from-outcome>
			<to-view-id>/newpages/ProzessverwaltungSuche.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung von Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungBearbeiten</from-outcome>
			<to-view-id>/newpages/ProzessverwaltungBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung der Werkstueckevon Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungBearbeitenWerkstueck</from-outcome>
			<to-view-id>/newpages/inc_Prozessverwaltung/werkstueck.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		
	<!-- Bearbeitung der Vorlagen von Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungBearbeitenVorlage</from-outcome>
			<to-view-id>/newpages/inc_Prozessverwaltung/vorlage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung der Schritte von Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungBearbeitenSchritt</from-outcome>
			<to-view-id>/newpages/inc_Prozessverwaltung/schritt.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- ++++++++++++++++++++++++ Neuen Vorgang anlegen ++++++++++++++++++++++++++++ -->
	
	<!-- Bearbeitung von Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungKopie1</from-outcome>
			<to-view-id>/newpages/NewProcess/Page1.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung von ProzessverwaltungKopie2 -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungKopie2</from-outcome>
			<to-view-id>/newpages/NewProcess/Page2.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		
	<!-- Bearbeitung von ProzessverwaltungKopie3 -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungKopie3</from-outcome>
			<to-view-id>/newpages/NewProcess/Page3.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- ++++++++++++++++++++++++ // Neuen Vorgang anlegen ++++++++++++++++++++++++++++ -->
	
	<!-- Aktuelle Schritte -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>AktuelleSchritteAlle</from-outcome>
			<to-view-id>/newpages/AktuelleSchritteAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Aktuelle Schritte - Bearbeiten -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>AktuelleSchritteBearbeiten</from-outcome>
			<to-view-id>/newpages/AktuelleSchritteBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Projekte -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProjekteAlle</from-outcome>
			<to-view-id>/newpages/ProjekteAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Projekte - Bearbeiten -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProjekteBearbeiten</from-outcome>
			<to-view-id>/newpages/ProjekteBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
					
    <!-- Filegroups für Projekt-->
    <navigation-rule>
        <navigation-case>
            <from-outcome>FilegroupPopup</from-outcome>
            <to-view-id>/newpages/ProjektFilegroupPopup.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>					
											
	<!-- Preferences -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>RegelsaetzeAlle</from-outcome>
			<to-view-id>/newpages/RegelsaetzeAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Preferences - Bearbeiten -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>RegelsaetzeBearbeiten</from-outcome>
			<to-view-id>/newpages/RegelsaetzeBearbeiten.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung der Benutzerberechtigungen für die Schritte -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BerechtigungBenutzerPopup</from-outcome>
			<to-view-id>/newpages/inc_Prozessverwaltung/schritt_box_BenutzerPopup.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Bearbeitung der Benutzergruppenberechtigungen für die Schritte -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>BerechtigungBenutzergruppenPopup</from-outcome>
			<to-view-id>/newpages/inc_Prozessverwaltung/schritt_box_BenutzergruppenPopup.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Alle Prozessverwaltung -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>ProzessverwaltungAlle</from-outcome>
			<to-view-id>/newpages/ProzessverwaltungAlle.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- aktive Benutzer -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>aktiveBenutzerNeu</from-outcome>
			<to-view-id>/newpages/aktiveNutzer.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- RuleSet converter for Select-Boxes -->
	<converter>
		<converter-id>RegelsatzConverter</converter-id>
		<converter-class>de.sub.goobi.converter.RegelsatzConverter</converter-class>
	</converter>
	
	<!-- StatisticsTimeUnitConverter for Select-Boxes -->
	<converter>
		<converter-id>StatisticsTimeUnitConverter</converter-id>
		<converter-class>de.sub.goobi.converter.StatisticsTimeUnitConverter</converter-class>
	</converter>
	
	<!-- StatisticResultOutputConverter for Select-Boxes -->
	<converter>
		<converter-id>StatisticsResultOutputConverter</converter-id>
		<converter-class>de.sub.goobi.converter.StatisticsResultOutputConverter</converter-class>
	</converter>
	
	<!-- StatisticCalculationUnitConverter for Select-Boxes -->
	<converter>
		<converter-id>StatisticsCalculationUnitConverter</converter-id>
		<converter-class>de.sub.goobi.converter.StatisticsCalculationUnitConverter</converter-class>
	</converter>
	
	<!-- Beans (formaly known as config_Beans.xml) -->
	
	<!-- BenutzerverwaltungForm -->
	<managed-bean>
		<managed-bean-name>BenutzerverwaltungForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.BenutzerverwaltungForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- PreferencesForm -->
	<managed-bean>
		<managed-bean-name>RegelsaetzeForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.RegelsaetzeForm </managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<!-- LdapGruppenForm -->
	<managed-bean>
		<managed-bean-name>LdapGruppenForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.LdapGruppenForm </managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<!-- BenutzergruppenForm -->
	<managed-bean>
		<managed-bean-name>BenutzergruppenForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.BenutzergruppenForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- LoginForm -->
	<managed-bean>
		<managed-bean-name>LoginForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.LoginForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- SpracheForm -->
	<managed-bean>
		<managed-bean-name>SpracheForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.SpracheForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- NavigationForm -->
	<managed-bean>
		<managed-bean-name>NavigationForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.NavigationForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- StatistikForm -->
	<managed-bean>
		<managed-bean-name>StatistikForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.StatistikForm </managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<!-- Bean für Administrationsaufgaben -->
	<managed-bean>
		<managed-bean-name>AdministrationForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.AdministrationForm </managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<!-- Bean für Prozessverwaltung -->
	<managed-bean>
		<managed-bean-name>ProzessverwaltungForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.ProzessverwaltungForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Bean für Projekte -->
	<managed-bean>
		<managed-bean-name>ProjekteForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.ProjekteForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Bean für Prozesskopie -->
	<managed-bean>
		<managed-bean-name>ProzesskopieForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.ProzesskopieForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Bean für Aktuelle Schritte -->
	<managed-bean>
		<managed-bean-name>AktuelleSchritteForm</managed-bean-name>
		<managed-bean-class> de.sub.goobi.Forms.AktuelleSchritteForm </managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Helper-Bean -->
	<managed-bean>
		<managed-bean-name>HelperForm</managed-bean-name>
		<managed-bean-class>de.sub.goobi.Forms.HelperForm</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- ModuleServer-Bean -->
	<managed-bean>
		<managed-bean-name>ModuleServerForm</managed-bean-name>
		<managed-bean-class>de.sub.goobi.Forms.ModuleServerForm</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- HibernateSessionLong-Bean -->
    <managed-bean>
       <managed-bean-name>HibernateSessionLong</managed-bean-name>
       <managed-bean-class>de.sub.goobi.Persistence.HibernateSessionLong</managed-bean-class>
       <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
	
	
</faces-config>

